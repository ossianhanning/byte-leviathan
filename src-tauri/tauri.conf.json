{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Byte Leviathan",
  "version": "0.1.0",
  "identifier": "com.byteleviathan.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "Byte Leviathan",
        "width": 1130,
        "height": 880,
        "resizable": true,
        "fullscreen": false,
        "devtools": true,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  }
}
